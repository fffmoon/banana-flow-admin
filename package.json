{
  "name": "BananaFlowAdmin",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@9.10.0",
  "homepage": "./",
  "author": {
    "name": "Qing git@FFFMOON"
  },
  "license": "MIT",
  "engines": {
    "pnpm": ">=9.0"
  },
  "scripts": {
    "serve": "npm run dev",
    "dev": "vite --mode development --host",
    "build:development": "vite build --mode development",
    "build:test": "npm run build:development",
    "build:production": "vite build --mode production",
    "build:time": "time vite build",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write ./src",
    "lint": "eslint . --no-warn-ignored",
    "lint:fix": "eslint . --fix --no-warn-ignored",
    "lint:output": "eslint . > log/eslint-report.txt",
    "lint:fix:output": "eslint . --fix > log/eslint-fix-report.txt 2>&1",
    "prepare": "husky install",
    "postversion": "git push && git push origin --tags",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm run dev",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "test:gzip": "npx http-server dist --cors --gzip -c-1",
    "test:br": "npx http-server dist --cors --brotli -c-1",
    "lint:lint-staged": "lint-staged",
    "lint:style": "stylelint --config stylelint.config.mjs --fix \"src/**/*.{vue,scss}\"",
    "lint:style:report": "stylelint --config stylelint.config.mjs \"src/**/*.{vue,scss}\" --formatter verbose"
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.1.0",
    "@antfu/utils": "^9.1.0",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify/vue": "^5.0.0",
    "@radix-ui/colors": "^3.0.0",
    "@tweenjs/tween.js": "^23.1.1",
    "@unocss/reset": "66.1.0-beta.5",
    "@vicons/ionicons5": "^0.13.0",
    "@vueuse/core": "^10.5.0",
    "axios": "^1.5.1",
    "cannon": "^0.6.2",
    "chroma-js": "^3.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "defu": "^6.1.4",
    "echarts": "^5.6.0",
    "lil-gui": "^0.20.0",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.482.0",
    "marked": "^12.0.2",
    "moment": "^2.30.1",
    "naive-ui": "^2.38.2",
    "nanoid": "^3.0.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "qs": "^6.11.2",
    "radix-vue": "^1.9.17",
    "reka-ui": "^2.3.2",
    "stylelint-config-recommended-scss": "^14.1.0",
    "tailwind-merge": "^3.0.2",
    "tiny-pinyin": "^1.3.2",
    "vconsole": "^3.15.1",
    "vite-plugin-app-loading": "^0.3.1",
    "vite-plugin-cdn-import": "^1.0.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@iconify-json/ion": "^1.2.2",
    "@iconify/utils": "^2.3.0",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.8.6",
    "@unocss/eslint-config": "^0.65.4",
    "@unocss/preset-icons": "0.65.4",
    "@unocss/preset-uno": "^0.65.4",
    "@unocss/preset-wind": "66.1.0-beta.5",
    "@vicons/fluent": "^0.12.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.19.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.69.3",
    "stylelint": "^15.10.3",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.2.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.2.2",
    "unocss": "^0.65.4",
    "unocss-preset-animations": "^1.1.1",
    "unocss-preset-shadcn": "^0.5.0",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "unplugin-vue-define-options": "3.0.0-beta.7",
    "vite": "^5.3.5",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vue-i18n": "11.1.2",
    "vue-tsc": "^1.8.19"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix --no-warn-ignored"
    ],
    "*.{css,scss,less,vue}": [
      "stylelint --config stylelint.config.mjs --fix"
    ]
  }
}
